	<subDataset name="Dataset1" uuid="9c999459-cffb-4c9c-96a7-9cd1db35ca01">
		
		<parameter name="id" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT   
	  b.bookingId  
	, b.bookingcost
    , b.seatReservationId
    , b.bookingdate
    , t.theatrename
    , c.cityname
    , u.email
    , u.fullName
    , m.moviename
FROM  bookings  AS b , shows AS s, theatres AS t,  cities AS c, users AS u, movies AS m
 where  b.showid = s.showid
    and s.theatreid = t.theatreid
    and t.cityid = c.cityid
    and b.userid = u.userid
    and s.movieid = m.movieid
    and b.userid = $P{id}]]>
		</queryString>
		<field name="email" class="java.lang.String"/>
		<field name="movie_name" class="java.lang.String"/>
		<field name="booking_date" class="java.sql.Date"/>
		<field name="seat_id" class="java.lang.Integer"/>
		<field name="booking_cost" class="java.math.BigDecimal"/>
		<field name="theatre_name" class="java.lang.String"/>
		<field name="city_name" class="java.lang.String"/>
	</subDataset>
